{
  "name": "@neomeprealpha/widget",
  "version": "0.0.3",
  "type": "module",
  "main": "./dist/index.cjs",
  "license": "MIT",
  "module": "./dist/index.js",
  "keywords": [
    "neome",
    "widget",
    "embed",
    "chat",
    "automation"
  ],
  "repository": "https://github.com/brijeshneoxl/neome-widget",
  "author": "neomeprealpha",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "readme.md"
  ],
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc --project tsconfig.build.json && vite build && yarn run copy:readme && yarn link && yarn link @neomeprealpha/widget",
    "preview": "vite preview",
    "copy:readme": "cpy readme.md dist",
    "upload": "yarn build && yarn publish"
  },
  "dependencies": {
    "preact": "^10.19.1"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.6.0",
    "@types/node": "^20.9.3",
    "cpy-cli": "^5.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.3"
  }
}
